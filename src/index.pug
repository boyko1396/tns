    script(src="js/chart.js?v=568532623346")
    script(src="js/PieMeister-with-Progress.min.js?v=568532623346")
    script(src="js/tingle.js?v=568532623346")
    script.
      // chart 
      var labels = ['Event 1', 'Event 2', 'Event 3', 'Event 4', 'Event 5', 'Event 6', 'Event 7', 'Event 8', 'Event 9', 'Event 10'];
      var player1Data = [5, 3, -2, 4, 0, -1, 2, 1, -3, 6];
      var player2Data = [-1, -3, 2, -4, 1, -2, 3, -5, 2, -6];

      var ctx = document.getElementById('playerChart').getContext('2d');
      var playerChart = new Chart(ctx, {
          type: 'line',
          data: {
              labels: labels,
              datasets: [{
                  label: '',
                  data: player1Data,
                  borderColor: '#17C6D2',
                  backgroundColor: '#17C6D2',
                  pointStyle: 'circle',
                  pointRadius: 5,
                  pointBorderColor: '#ffffff',
                  pointBorderWidth: 2,
                  hoverOffset: 14
              }, {
                  label: '',
                  data: player2Data,
                  borderColor: '#FC8171',
                  backgroundColor: '#FC8171',
                  pointStyle: 'circle',
                  pointRadius: 5,
                  pointBorderColor: '#ffffff',
                  pointBorderWidth: 2,
                  hoverOffset: 14
              }]
          },
          options: {
              responsive: true,
              scales: {
                  x: {
                      display: false
                  },
                  y: {
                      display: true,
                      position: 'right',
                      title: {
                          display: false
                      },
                      grid: {
                          drawBorder: false,
                          drawOnChartArea: false,
                          tickWidth: 0
                      },
                      suggestedMin: -6,
                      suggestedMax: 6
                  }
              },
              plugins: {
                  tooltip: {
                      enabled: false,
                      external: customTooltip
                  },
                  legend: {
                    display: false
                  }
              },
              interaction: {
                  intersect: false,
                  mode: 'nearest'
              }
          }
      });

      function customTooltip(context) {
          var { chart, tooltip } = context;
          var { dataPoints } = tooltip;

          if (dataPoints && dataPoints.length) {
              var dataPoint = dataPoints[0];
              var index = dataPoint.dataIndex;

              var { x, y } = chart.getDatasetMeta(dataPoint.datasetIndex).data[index].getCenterPoint();

              var tooltipContainer = document.getElementById('tooltip-container');

              var winClass = player1Data[index] > player2Data[index] ? 'is-win' : '';
              var loseClass = player1Data[index] < player2Data[index] ? 'is-lose' : '';

              tooltipContainer.innerHTML = `
                  <div class="tooltip-result-chart ${winClass} ${loseClass}">
                      <div class="tooltip-result-chart__inner">
                          <header class="tooltip-result-chart__header">
                              <div class="tooltip-result-chart__title">1:0 (5:7) (40:15)</div>
                              <div class="tooltip-result-chart__title">Выиграл</div>
                          </header>
                          <div class="tooltip-result-chart__body">
                              <picture class="tooltip-result-chart__picture"><img src="images/player-1.jpg" /></picture>
                              <div class="tooltip-result-chart__content">
                                  <div class="tooltip-result-chart__title">${labels[index]}</div><time class="tooltip-result-chart__date" datetime="2019-01-24">24.01.19</time>
                              </div>
                          </div>
                      </div>
                  </div>
              `;

              tooltipContainer.style.left = chart.canvas.offsetLeft + x + 'px';
              tooltipContainer.style.top = chart.canvas.offsetTop + y + 'px';
              tooltipContainer.style.display = 'block';
          } else {
              document.getElementById('tooltip-container').style.display = 'none';
              playerChartCircle.update();
          }
      }

      // chart circle
      var ctx = document.getElementById('playerChartCircle').getContext('2d');
      var playerChartCircle = new Chart(ctx, {
        type: 'doughnut',
        data: {
          labels: ['Грунт', 'Трава', 'Ковер', 'Хард'],
          datasets: [{
            data: [30, 25, 20, 25],
            backgroundColor: ['#F39E3A', '#93CE47', '#6F8AE8', '#B88BE5'],
          }]
        },
        options: {
          cutoutPercentage: 80,
          responsive: true,
          animation: {
            animateRotate: false,
            animateScale: true
          },
          plugins: {
            legend: {
              position: 'right',
              labels: {
                generateLabels: function(chart) {
                  var data = chart.data;
                  if (data.labels.length && data.datasets.length) {
                    return data.labels.map(function(label, index) {
                      var dataset = data.datasets[0];
                      var backgroundColor = dataset.backgroundColor[index];
                      return {
                        text: label,
                        fillStyle: backgroundColor,
                        strokeStyle: backgroundColor,
                        index: index
                      };
                    });
                  }
                  return [];
                }
              },
              onHover: function(evt, legendItem) {
                document.getElementById('playerChartCircle').style.cursor = 'pointer';
              },
              onLeave: function(evt, legendItem) {
                document.getElementById('playerChartCircle').style.cursor = 'default';
              }
            }
          },
          tooltips: {
            enabled: false
          }
        }
      });

      // event card stat show
      const eventInfoBtn = document.querySelectorAll('.js-event-card-stat-view');

      eventInfoBtn.forEach(function(button) {
        button.addEventListener('click', function() {
          var eventCard = this.closest('.event-card');
          eventCard.classList.toggle('is-show-stat');
        });
      });

      // event card stat show mobile
      const eventInfoBtnMobile = document.querySelectorAll('.js-event-card-stat-view-mobile');

      eventInfoBtnMobile.forEach(function(button) {
        button.addEventListener('click', function() {
          var eventCard = this.closest('.event-card');
          document.body.classList.toggle('is-hidden-mobile');
          eventCard.classList.toggle('is-show-stat-mobile');
        });
      });

      const eventInfoBtnMobileClose = document.querySelectorAll('.js-event-versus-btn-close');

      eventInfoBtnMobileClose.forEach(function(button) {
        button.addEventListener('click', function() {
          var eventCard = this.closest('.event-card');
          document.body.classList.remove('is-hidden-mobile');
          eventCard.classList.remove('is-show-stat-mobile');
        });
      });

      // event card graph show mobile
      const eventGraphBtnMobile = document.querySelectorAll('.js-event-card-graph-mobile');

      eventGraphBtnMobile.forEach(function(button) {
        button.addEventListener('click', function() {
          var eventCard = this.closest('.event-card');
          document.body.classList.toggle('is-hidden-mobile');
          eventCard.classList.toggle('is-show-graph-mobile');
        });
      });

      const eventGraphBtnMobileClose = document.querySelectorAll('.js-event-graph-btn-close');

      eventGraphBtnMobileClose.forEach(function(button) {
        button.addEventListener('click', function() {
          var eventCard = this.closest('.event-card');
          document.body.classList.remove('is-hidden-mobile');
          eventCard.classList.remove('is-show-graph-mobile');
        });
      });